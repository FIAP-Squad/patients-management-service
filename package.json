{
  "name": "doctors-management-service",
  "version": "1.1.0",
  "description": "This service is responsible for user management and token management.",
  "main": "dist/main/server.js",
  "scripts": {
    "start": "npm run build && node dist/main/server.js",
    "start:dev": "ts-node src/main/server.ts",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "husky:prepare": "husky install",
    "lint:fix": "eslint 'src/**' --fix",
    "lint-staged:run": "npx lint-staged",
    "check": "npm-check -s -u",
    "test": "jest --passWithNoTests --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:failures": "jest --onlyFailures",
    "changelog:generate": "standard-version",
    "test:ci": "npm test -- --coverage"
  },
  "keywords": [],
  "author": "Gabriel Rodrigues",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.3.2",
    "@types/cli-color": "^2.0.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^5.0.6",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "module-alias": "^2.2.3",
    "rimraf": "^5.0.5",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.658.1",
    "@prisma/client": "^5.20.0",
    "amqplib": "^0.10.4",
    "axios": "^1.7.7",
    "cli-color": "^2.0.3",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.8.0",
    "prisma": "^5.20.0",
    "validator": "^13.11.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
